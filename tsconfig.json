{
    "compilerOptions": {
      "target": "ESNext",
      "baseUrl": ".",
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": [
      "**/*.ts",
      "**/*.tsx",
      "**/*.jsx"
    ],
    "exclude": [
      "./fixtures/**/*.*",
      "./_fixtures/**/*.*"
    ]
  }